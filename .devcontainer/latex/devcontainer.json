{
  "name": "LaTeX Development",
  "dockerComposeFile": [
    "../../docker-compose.yml",
    "../docker-compose.dev.yml"
  ],
  "service": "latex",
  "workspaceFolder": "/workspace",
  "customizations": {
    "vscode": {
      "extensions": [
        "James-Yu.latex-workshop"
      ],
      "settings": {
        "editor.formatOnSave": true,
        "latex-workshop.latex.autoBuild.run": "onSave"
      }
    }
  },
  "remoteUser": "vscode"
}
